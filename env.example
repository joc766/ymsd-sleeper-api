# Environment variables for YMSD Sleeper API
# Copy this file to .env and update the values

# AWS Configuration
AWS_REGION=us-east-1
S3_BUCKET_NAME=ymsd-football
S3_PREFIX=sleeper-snapshots/

# EFS Configuration
EFS_MOUNT_PATH=/mnt/efs

# Version Management
VERSION_MANAGEMENT_SERVICE_URL=https://api.ymsd-sleeper.com/version-management

# Infrastructure (set by deployment scripts)
EFS_SECURITY_GROUP_ID=
EFS_SUBNET_ID_1=
EFS_SUBNET_ID_2=
EFS_ACCESS_POINT_ARN=

# Development
LOG_LEVEL=INFO
